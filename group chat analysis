postive=sum(data1['positive'])
Negative=sum(data1['negative'])
neutral=sum(data1['neutral']) ## sum of polarity scores
if postive<Negative or postive>neutral:
    print("postive")
elif postive<neutral or Negative<neutral:
    print("netural")
else:
    print("negative")
    
